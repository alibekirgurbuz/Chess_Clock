{
  "expo": {
    "name": "Chess Clock",
    "slug": "ReactNativeAntrenman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chess-clock.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark"
    },
